{
  "compilerOptions": {
    "target": "es5",
    "types": [
      "react/next",
      "react-dom/next"
    ],
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "jsxFactory": "",
    "jsxFragmentFactory": "",
    "baseUrl": "./"
  },
  "include": ["src"],
  "exclude": ["node_modules", "dist", "example", "docs"]
}
